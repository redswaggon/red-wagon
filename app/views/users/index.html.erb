<ul>
  <% @users.each do |user| %>
    <li><%= link_to user.username, user_path(user) %></li>
    
    <ul>
      <div class="photos">
        <% user.items.each do |item| %>
          <li> Item: <%= image_tag item.photo.url(:square) %></li>
        <% end %>
      </div>
    </ul>
  <% end %>
</ul>